<div id="mc_embed_signup">
  <form
    action="https://gmail.us1.list-manage.com/subscribe/post?u=7accc9b7f018fb5ae1e19e6ad&amp;id=f0438e2c06"
    method="post"
    id="mc-embedded-subscribe-form"
    name="mc-embedded-subscribe-form"
    class="validate js-signup-form"
    target="_blank"
    novalidate
  >
    <div
      id="mc_embed_signup_scroll"
      class="signup-form d-flex flex-column justify-content-between"
    >
      <h3 class="mb-4">
        Signup for updates
      </h3>
      <div class="mc-field-group mb-3">
        <label for="mce-FNAME" class="form-label">First Name <span class="asterisk text-danger">*</span></label>
        <input
          type="text"
          value=""
          name="FNAME"
          class="required form-control js-fname"
          id="mce-FNAME"
          placeholder="John"
          required
        >
      </div>
      <div class="mc-field-group mb-3">
        <label for="mce-EMAIL" class="form-label">Email <span class="asterisk text-danger">*</span></label>
        <input
          type="email"
          value=""
          name="EMAIL"
          class="required email form-control js-email"
          id="mce-EMAIL"
          placeholder="john@example.com"
          required
        >
      </div>
      <div id="mce-responses">
        <div class="response" id="mce-error-response" style="display:none"></div>
        <div class="response" id="mce-success-response" style="display:none"></div>
      </div>
      <div style="position: absolute; left: -5000px;" aria-hidden="true">
        <input type="text" name="b_7accc9b7f018fb5ae1e19e6ad_f0438e2c06" tabindex="-1" value="">
      </div>
      <div>
        <input
          type="submit"
          value="Signup"
          name="subscribe"
          id="mc-embedded-subscribe"
          class="btn btn-warning signup-form__button js-submit-button"
        >
      </div>
    </div>
  </form>
</div>
